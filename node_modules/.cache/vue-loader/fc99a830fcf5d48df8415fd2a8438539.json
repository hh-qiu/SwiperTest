ver=\"stop\" @mouseout=\"start\">\n    <ul ref=\"carul\" class=\"car_content\" id=\"picture\">\n      <li v-for=\"(banner,index) in bannerList\" :key=\"index\" :style=\"{width:liwidth+'px'}\">\n        <p>{{banner.page}}</p>\n        <p>{{banner.content}}</p>\n      </li>\n    </ul>\n    <div class=\"page\">\n      \n    </div>\n    <a href=\"javascript:;\" class=\"arrow arrow_left\" @click=\"prev\">xia</a>\n    <a href=\"javascript:;\" class=\"arrow arrow_right\" @click=\"next\">></a>\n  </div>\n</template>\n<script>\n// import { clearInterval, setInterval } from \"timers\";\n// import {Debounce,Throttle} from '@/uilt/Antishake'\nvar playtimer;\nexport default {\n  name: \"SwiperTest\",\n  data() {\n    return {\n      mark: 0,\n      index: 0,\n      idx:0,\n      leftL: 0,\n      liwidth: 0,\n      bannerList: [\n        {\n          page: 1,\n          content: [\n            {\n              // img:require(),\n              url: \"www.baidu.com\",\n              des: \"倚天屠龙记\",\n              type: \"电视剧\"\n            },\n            {\n              // img:require(),\n              url: \"www.baidu.com\",\n              des: \"王牌对王牌\",\n              type: \"综艺\"\n            },\n            {\n              // img:require(),\n              url: \"www.baidu.com\",\n              des: \"澳门风云\",\n              type: \"电影\"\n            },\n            {\n              // img:require(),\n              url: \"www.baidu.com\",\n              des: \"斗罗大陆\",\n              type: \"动漫\"\n            }\n          ]\n        },\n        {\n          page: 2,\n          content: [\n            {\n              // img:require(),\n              url: \"www.baidu.com\",\n              des: \"倚天屠龙记\",\n              type: \"电视剧\"\n            },\n            {\n              // img:require(),\n              url: \"www.baidu.com\",\n              des: \"王牌对王牌\",\n              type: \"综艺\"\n            },\n            {\n              // img:require(),\n              url: \"www.baidu.com\",\n              des: \"澳门风云\",\n              type: \"电影\"\n            },\n            {\n              // img:require(),\n              url: \"www.baidu.com\",\n              des: \"斗罗大陆\",\n              type: \"动漫\"\n            }\n          ]\n        },\n        {\n          page: 3,\n          content: [\n            {\n              // img:require(),\n              url: \"www.baidu.com\",\n              des: \"倚天屠龙记\",\n              type: \"电视剧\"\n            },\n            {\n              // img:require(),\n              url: \"www.baidu.com\",\n              des: \"王牌对王牌\",\n              type: \"综艺\"\n            },\n            {\n              // img:require(),\n              url: \"www.baidu.com\",\n              des: \"澳门风云\",\n              type: \"电影\"\n            },\n            {\n              // img:require(),\n              url: \"www.baidu.com\",\n              des: \"斗罗大陆\",\n              type: \"动漫\"\n            }\n          ]\n        }\n      ],\n    };\n  },\n  computed: {\n  num:function(){\n    return this.mark == this.bannerList.length-1 ? 0 : this.mark;\n  },\n  len() {\n    var len = this.bannerList.length;\n    return len;\n  },\n  },\n  created() {\n    this.bannerList.push(this.bannerList[0]);\n    this.autoplay();\n  },\n  mounted() {\n    var carWidth = this.$refs.car.clientWidth;\n    this.liwidth = carWidth;\n    var ClientLeft = this.$refs.carul.getBoundingClientRect().left;\n    this.leftL = ClientLeft;\n  },\n  methods: {\n    next() {\n      if (this.mark == this.len - 1) {\n        this.mark = 0;\n        this.$refs.carul.style.marginLeft = 0 + \"px\";\n      }\n      this.mark++;\n      var distance = -this.mark * this.liwidth;\n      this.animate(distance);\n      this.index++;\n      if (this.index > this.len - 2) {\n        this.index = 0;\n      }\n    },\n    prev() {\n      if (this.mark == 0) {\n        this.mark = this.len - 1;\n        this.$refs.carul.style.marginLeft = -this.mark * this.liwidth + \"px\";\n      }\n      this.mark--;\n      var distance = -this.mark * this.liwidth;\n      this.animate(distance);\n      this.index--;\n      if (this.index < 0) {\n        this.index = this.len - 2;\n      }\n    },\n    autoplay() {\n      playtimer = setInterval(() => {\n         this.next();\n      }, 1000);\n    },\n    stop() {\n      clearInterval(playtimer);\n    },\n    start(){\n         this.autoplay()\n    },\n    // Choice:Debounce(function(idx){\n    //         this.idx = idx\n    //         this.mark=idx-1;\n    //         this.next();\n    // }),\n    animate(distance) {\n      clearInterval(animatetimer);\n      var animatetimer = setInterval(() => {\n        var pres = this.$refs.carul.getBoundingClientRect().left;\n        var present = pres - this.leftL; //获取元素的当前的位置\n        var movement = 20; //每次移动的距离\n        movement = present < distance ? movement : -movement;\n        present += movement; //当前移动到位置\n        if (Math.abs(present - distance) > Math.abs(movement)) {\n          this.$refs.carul.style.marginLeft = present + \"px\";\n        } else {\n          clearInterval(animatetimer);\n          this.$refs.carul.style.marginLeft = distance + \"px\";\n        }\n      }, 10);\n    }\n  }\n};\n</script>\n<style scoped>\n.carousel {\n  width: 100%;\n  height: 100%;\n  position: relative;\n  overflow: hidden;\n  border: 2px solid rgba(189, 184, 184, 0.8);\n  z-index: 1;\n}\n.car_content {\n  position: absolute;\n  width: 1000%;\n  height: 100%;\n}\n.car_content li {\n  height: 100%;\n  float: left;\n  cursor: pointer;\n}\n.car_content li:hover {\n  opacity: 0.5;\n}\n.carousel .page {\n  position: absolute;\n  left: 50%;\n  transform: translateX(-50%);\n  bottom: 5px;\n  z-index: 2;\n}\n.carousel .page span {\n  display: inline-block;\n  width: 30px;\n  height: 5px;\n  width: 30px;\n  margin-left: 10px;\n  color: white;\n  background: rgba(111, 112, 112, 0.6);\n  cursor: pointer;\n}\n.carousel .page span:last-child {\n  display: none;\n}\n.carousel .arrow {\n  cursor: pointer;\n  position: absolute;\n  top: 40%;\n  padding: 10px 15px;\n  background-color: rgba(255, 255, 255, 0.8);\n  display: inline-block;\n  font-size: 50px;\n  z-index: 2;\n  color: rgb(161, 157, 157);\n  font-weight: bold;\n  display: none;\n}\n.carousel .arrow_left {\n  left: 0;\n}\n\n.carousel .arrow_right {\n  right: 0;\n}\n.carousel .arrow_right {\n  right: 0;\n}\n\n.carousel:hover .arrow {\n  display: block;\n}\n.carousel .arrow:hover {\n  background-color: rgb(15, 151, 241);\n  font-weight: bold;\n  color: white;\n}\n.active {\n  background: rgb(15, 151, 241) !important;\n}\n.car_content li:nth-child(1) {\n  background-color: rgb(20, 247, 216);\n}\n.car_content li:nth-child(2) {\n  background-color: rgb(247, 111, 20);\n}\n.car_content li:nth-child(3) {\n  background-color: rgb(247, 20, 179);\n}\n.car_content li:nth-child(4) {\n  background-color: rgb(20, 247, 216);\n}\n</style>\n"]}]}